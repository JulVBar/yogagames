
mixin pageTitle(title)
  .page-title
      h1(data-animation)= title


mixin eventService(image, text)
  .event-service__item.small-section.events-fade-anim
    .event-service__item-inner.events-slide-anim
      .event-service__icon.image-contain
        img(src=image, alt="")
      .event-service__text= text